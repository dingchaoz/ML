    "# need to look at on how to place multiple plots side by side http://worksofscience.net/matplotlib/gridspec\n",
    "frequencies = []\n",
    "counts = df.grade.value_counts()\n",
    "keys = list(dict(counts).keys())\n",
    "keys.sort()\n",
    "for each in keys:\n",
    "    frequencies.append(dict(counts).get(each))\n",
    "\n",
